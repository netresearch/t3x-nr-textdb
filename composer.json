{
    "name": "netresearch/nr-textdb",
    "type": "typo3-cms-extension",
    "description": "Allows you to edit the translations in the back end",
    "authors": [
        {
            "name": "Thomas Sch√∂ne",
            "email": "thomas.schoene@netresearch.de",
            "role": "Developer",
            "homepage": "https://www.netresearch.de/"
        },
        {
            "name": "Axel Seemann",
            "email": "axel.seemann@netresearch.de",
            "role": "Developer",
            "homepage": "https://www.netresearch.de/"
        },
        {
            "name": "Tobias Hein",
            "email": "tobias.hein@netresearch.de",
            "role": "Developer",
            "homepage": "https://www.netresearch.de/"
        },
        {
            "name": "Rico Sonntag",
            "email": "rico.sonntag@netresearch.de",
            "role": "Developer",
            "homepage": "https://www.netresearch.de/"
        }
    ],
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true,
            "phpro/grumphp": true
        }
    },
    "require": {
        "php": "^8.1",
        "ext-zip": "*",
        "ext-simplexml": "*",
        "ext-libxml": "*",
        "typo3/cms-core": "^12.4",
        "typo3/cms-extbase": "^12.4",
        "typo3/cms-extensionmanager": "^12.4"
    },
    "require-dev": {
        "typo3/testing-framework": "^8.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "squizlabs/php_codesniffer": "^3.8",
        "phpro/grumphp": "^2.4",
        "ssch/typo3-rector": "^1.8"
    },
    "suggest": {
        "netresearch/nr-sync": "*"
    },
    "autoload": {
        "psr-4": {
            "Netresearch\\NrTextdb\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netresearch\\NrTextdb\\Tests\\": "Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "typo3/cms": {
            "extension-key": "nr_textdb"
        }
    }
}
